    public MoveBall(InputActionAsset asset)
        : base(asset)
    {
    }
    private bool m_Initialized;
    private void Initialize()
    {
        // Ball
        m_Ball = asset.GetActionMap("Ball");
        m_Ball_Movement = m_Ball.GetAction("Movement");
        m_Initialized = true;
    }
